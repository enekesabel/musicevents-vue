{
  "defaultSeverity": "warning",
  "extends": [
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "no-increment-decrement": [
      false
    ],
    "object-curly-spacing": [
      false
    ],
    "max-line-length": [
      true,
      180
    ],
    "import-name": [
      false
    ],
    "align": [
      false,
      "arguments"
    ],
    "function-name": [
      true,
      {
        "method-regex": "^[\\[a-zA-Z][\\.\\]\\w\\d]+$"
      }
    ]
  }
}
